{
  "name": "altinity-dashboard",
  "version": "0.1.1",
  "repository": "https://github.com/altinity/altinity-dashboard.git",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "devel": "concurrently \"../adash -devmode -notoken\" \"npm run start:dev\"",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.prod.js",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "install-git-hooks": "cd .. && husky install ui/.husky"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^27.0.3",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.4",
    "imagemin": "^8.0.0",
    "jest": "^27.4.3",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^2.2.0",
    "react-router-dom": "^5.3.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.3",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.2.0",
    "typescript": "4.5.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@patternfly/react-charts": "^6.28.4",
    "@patternfly/react-code-editor": "^4.16.4",
    "@patternfly/react-core": "^4.162.3",
    "@patternfly/react-icons": "^4.13.1",
    "@patternfly/react-styles": "^4.12.5",
    "@patternfly/react-table": "^4.37.8",
    "concurrently": "^6.3.0",
    "jssha": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-monaco-editor": "^0.41.2",
    "react-router-last-location": "git://github.com/hinok/react-router-last-location#2d57c676abb49f3f5755c44fbe2c24b01a8bb68b",
    "sirv-cli": "^1.0.12",
    "stream": "^0.0.2",
    "swagger-ui-react": "^4.1.2"
  }
}
